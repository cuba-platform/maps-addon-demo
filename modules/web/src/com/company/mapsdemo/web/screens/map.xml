<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://caption"
        class="com.company.mapsdemo.web.screens.Map"
        messagesPack="com.company.mapsdemo.web.screens"
        xmlns:maps="http://schemas.haulmont.com/maps/0.1/ui-component.xsd">
    <dialogMode height="600"
                width="800"/>
    <layout>
        <maps:geoMap id="map" height="100%" width="100%" zoom="3">
            <maps:layers>
                <maps:tile id="tiles"
                           urlPattern="https://{s}.basemaps.cartocdn.com/rastertiles/voyager/{z}/{x}/{y}{r}.png"
                           attribution="CARTO"/>
            </maps:layers>
        </maps:geoMap>
    </layout>
</window>
